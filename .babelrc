{
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties"
      ],
      "presets": [
        "env",
        "es2015",
        "react",
        "stage-2"
      ]
    },
    "production": {
      "plugins": [
        "babel-loader",
        "transform-remove-console"
      ],
      "presets": [
        "es2016",
        "es2015",
        "react",
        "stage-2"
      ]
    },
    "test": {
      "plugins": [
        "transform-class-properties"
      ],
      "presets": [
        "jest",
        "es2015",
        "react",
        "stage-2"
      ]
    }
  }
}
